<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Damilola Odelola" %></title>
    <%= stylesheet_link_tag 'normalise' %>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <%= stylesheet_link_tag 'general' %>
    <%= stylesheet_link_tag 'rough_ideas' %>
    <%= stylesheet_link_tag 'rough_ideas_post' %>
    <script src="javascripts/site.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400" rel="stylesheet">
  </head>
  <body>
    <div class="container" >
      <div  class="row">
        <header class="col-md-6">
          <div class="nav"> LOLA: <a href="/"> HOME </a></div>
          <% link_to '/rough_ideas.html' do %>
            <%= image_tag "rough_ideas_logo.svg" %>
          <% end %>
        </header>
      </div>
      <%= yield %>
    </div>
    <footer>
     <div class="social-media">

         <a href="https://twitter.com/lolaodelola">
           <%= image_tag "twitter_icon.svg" %>
         </a>

       <%#= image_tag "fb_icon.svg" %>

         <a href="https://instagram.com/lolaodelola">
           <%= image_tag "ig_icon.svg" %>
         </a>

     </div>
     <div class="copyright"> Damilola Odelola &copy; <%= Date.today.year %> </div>
    </footer>
  </body>
</html>
